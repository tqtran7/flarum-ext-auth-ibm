{
    "name": "tqtran7/flarum-ext-auth-ibm",
    "description": "Allow users to log in with IBM credentials.",
    "minimum-stability": "dev",
    "type": "flarum-extension",
    "keywords": ["authentication"],
    "license": "MIT",
    "authors": [
        {
            "name": "Thai Tran",
            "email": "tqtran@us.ibm.com"
        }
    ],
    "support": {
        "issues": "https://github.com/tqtran7/flarum-ext-auth-ibm/issues",
        "source": "https://github.com/tqtran7/flarum-ext-auth-ibm"
    },
    "require": {
        "flarum/core": "^0.1.0-beta.6"
    },
    "autoload": {
        "psr-4": {
            "Flarum\\Auth\\IBM\\": "src/"
        }
    },
    "extra": {
        "flarum-extension": {
            "title": "IBM SSO",
            "icon": {
                "name": "ibm",
                "backgroundColor": "#eee",
                "color": "#333"
            }
        }
    }
}
